<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-autowire="constructor">

    <bean id="buildScanViewBuildTab"
          class="com.gradle.develocity.BuildScanViewBuildTab"
          destroy-method="unregister">
    </bean>

    <bean id="buildScanBuildServerListener"
          class="com.gradle.develocity.BuildScanBuildServerListener"
          init-method="register"
          destroy-method="unregister">
    </bean>

    <bean id="buildScanCrumbSummaryExtension"
          class="com.gradle.develocity.BuildScanCrumbSummaryExtension"
          init-method="register"
          destroy-method="unregister">
    </bean>

    <bean id="cachedBuildScanLookup" primary="true"
          class="com.gradle.develocity.internal.CachedBuildScanLookup"
          destroy-method="destroy">
        <constructor-arg type="com.gradle.develocity.BuildScanLookup" ref="dataStoreBuildScanLookup"/>
    </bean>

    <bean id="dataStoreBuildScanLookup"
          class="com.gradle.develocity.internal.DataStoreBuildScanLookup">
        <constructor-arg type="com.gradle.develocity.BuildScanLookup" ref="logIteratingBuildScanLookup"/>
    </bean>

    <bean id="logIteratingBuildScanLookup"
          class="com.gradle.develocity.internal.LogIteratingBuildScanLookup"/>

    <bean id="buildScanServiceMessageListener"
          class="com.gradle.develocity.internal.BuildScanServiceMessageListener"/>

    <bean id="artifactBuildScanDataStore"
          class="com.gradle.develocity.internal.ArtifactBuildScanDataStore"/>

    <bean id="buildScanDisplayArbiter"
          class="com.gradle.develocity.internal.DefaultBuildScanDisplayArbiter"/>

    <bean id="slackIntegration"
          class="com.gradle.develocity.internal.slack.SlackIntegration"
          destroy-method="shutdown"/>

    <bean id="gradleEnterpriseConnectionProvider"
          class="com.gradle.develocity.connection.DevelocityConnectionProvider">
    </bean>

    <bean id="gradleEnterpriseParametersProvider"
          class="com.gradle.develocity.connection.DevelocityParametersProvider">
    </bean>

    <bean id="gradleEnterprisePasswordProvider"
          class="com.gradle.develocity.connection.DevelocityPasswordProvider">
    </bean>

</beans>
